[core]
	editor = vi
[init]
	defaultBranch = main
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
[rerere]
	enabled = true
[maintenance]
	auto = false
	strategy = incremental
[alias]
	adog = log --all --decorate --oneline --graph
	co = checkout
	s = status --short
	update = commit --all --message update
	wip = commit --all --message wip
