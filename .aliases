# Aesthetic aliases
alias asciiquarium="perl /usr/local/bin/asciiquarium"

# Cmus stuff
alias cmn="cmus-remote --next"
alias cmr="cmus-remote --previous"
alias cmp="cmus-remote --play"
alias cmu="cmus-remote --pause"
alias cmq="cmus-remote --query"

# Tmux stuff
alias tas="tmux attach-session"
alias tat="tmux attach-session -t"
alias tkt="tmux kill-session -t"
alias tls="tmux list-sessions"
alias tns="tmux new -s"

# Misc stuff
alias bat=batcat
